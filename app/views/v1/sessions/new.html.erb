<h1>Sign In</h1>

<%= form_with(url: v1_sign_in_path, method: :post) do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, required: true %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password, required: true %>
  </div>

  <div>
    <%= form.submit "Sign In" %>
  </div>
<% end %>